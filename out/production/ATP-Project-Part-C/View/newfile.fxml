<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="279.0" prefWidth="370.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.newFile">
   <children>
      <Button layoutX="54.0" layoutY="133.0" mnemonicParsing="false" onAction="#generate" prefHeight="53.0" prefWidth="254.0" style="-fx-background-color: #d922ab;" text="Ganerate Maze">
         <effect>
            <Glow level="0.53" />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Stencil" size="24.0" />
         </font>
      </Button>
      <Button layoutX="132.0" layoutY="206.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="117.0" text="Random Size" />
      <TextField fx:id="tfRow" layoutX="171.0" layoutY="39.0" prefHeight="25.0" prefWidth="117.0"  />
      <TextField fx:id="tfCol" layoutX="181.0" layoutY="79.0" prefHeight="27.0" prefWidth="117.0" />
      <Label layoutX="99.0" layoutY="38.0" text="Rows" textFill="#d922ab">
         <font>
            <Font name="Stencil" size="22.0" />
         </font>
      </Label>
      <Label layoutX="77.0" layoutY="79.0" text="Columns" textFill="#d922ab">
         <font>
            <Font name="Stencil" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>





