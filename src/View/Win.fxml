<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" style="-fx-background-image: url(Images/WINN.gif); -fx-background-size: 100% 100%;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.WinController">
        <children>
            <Button layoutX="80.0" layoutY="450.0" mnemonicParsing="false" onAction="#PlayAgain" prefHeight="86.0" prefWidth="354.0" style="-fx-background-color: transparent; -fx-cursor: hover;" stylesheets="@Win.css" text="Play Again">
                <font>
                    <Font name="Stencil" size="45.0" />
                </font>
                <effect>
                    <DropShadow />
                </effect></Button>
            <Button layoutX="394.0" layoutY="450.0" mnemonicParsing="false" onAction="#Quit" prefHeight="86.0" prefWidth="354.0" style="-fx-background-color: transparent; -fx-cursor: hover;" stylesheets="@Win.css" text="Quit Game">
                <font>
                    <Font name="Stencil" size="45.0" />
                </font>
                <effect>
                    <DropShadow />
                </effect></Button>
        </children>
    </AnchorPane>
